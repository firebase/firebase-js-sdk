{
  // Ruler can't be specified via editorconfig
  "editor.rulers": [100],

  "search.exclude": {
    // Exclude gulp build outputs from search results
    "dist": true,

    // Exclude installed dependencies from searches too
    "**/node_modules": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.associations": {
    "*.json": "jsonc"
  },
  "cSpell.words": [
    "unregisters"
  ]
}
