{
  "extends": "../../config/tsconfig.base.json",
  "compilerOptions": {
    "lib": ["es2017", "dom"],
    "downlevelIteration": true,
    "declaration": false /* Declarations are handled by dts-bundle-generator. */,

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "exclude": ["dist"]
}
