{
    "extends": "../../config/.eslintrc.json",
    "parserOptions": {
        "project": "tsconfig.json"
    },
    "plugins": [
        "import"
    ],
    "rules": {
        "no-console":[ "error", { "allow": ["warn", "error"] }],
        "import/no-default-export": "error",
        "@typescript-eslint/no-unused-vars": [
            "error",
            {
                "varsIgnorePattern": "^_",
                "args": "none"
            }
        ]
    },
    "overrides": {
        "files": [
            "*test.ts",
            "**/test/**/*.ts"
        ],
        "rules": {
            "no-console": "off"
        }
    }
}