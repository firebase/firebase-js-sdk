{
  "name": "@firebase/auth-ts",
  "version": "0.14.0",
  "description": "Typescript library for Firebase Authentication SDK",
  "author": "Firebase <firebase-support@google.com> (https://firebase.google.com/)",
  "license": "Apache-2.0",
  "main": "dist/index.node.cjs.js",
  "browser": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "esm2017": "dist/index.esm2017.js",
  "react-native": "dist/index.rn.cjs.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint -c .eslintrc.js '**/*.ts' --ignore-path '../../.gitignore'",
    "lint:fix": "eslint --fix -c .eslintrc.js '**/*.ts' --ignore-path '../../.gitignore'",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "run-p lint test:browser test:node",
    "test:browser": "karma start --single-run",
    "test:browser:debug": "karma start --browsers=Chrome --auto-watch",
    "test:node": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' nyc --reporter lcovonly -- mocha test/**/*.test.* --opts ../../config/mocha.node.opts",
    "prepare": "yarn build",
    "serve": "node ./test/index.js"
  },
  "repository": {
    "directory": "packages/auth-ts",
    "type": "git",
    "url": "git+https://github.com/firebase/firebase-js-sdk.git"
  },
  "typings": "dist/index.d.ts",
  "keywords": [
    "Firebase",
    "authentication",
    "Firebase Auth",
    "Cloud Identity",
    "Google Cloud"
  ],
  "bugs": {
    "url": "https://github.com/firebase/firebase-js-sdk/issues"
  },
  "homepage": "https://github.com/firebase/firebase-js-sdk#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "mocha": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "sourcemap": "^0.1.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5"
  },
  "peerDependencies": {
    "@firebase/app": "0.x"
  },
  "dependencies": {
    "@firebase/util": "^0.2.36",
    "@types/grecaptcha": "^3.0.0",
    "tslib": "^1.10.0"
  }
}
