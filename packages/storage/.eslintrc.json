{
    "extends": "../../config/.eslintrc.json",
    "parserOptions": {
        "project": "tsconfig.json"
    },
    "rules": {
        "no-throw-literal": "off",
        "@typescript-eslint/no-unused-vars": [
            "error",
            {
                "varsIgnorePattern": "^_",
                "args": "none"
            }
        ]
    }
}